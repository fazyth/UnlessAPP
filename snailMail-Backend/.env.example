# Google Maps API Key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Anthropic Claude API Key (fallback)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Server Configuration
PORT=3001
FRONTEND_URL=http://localhost:5173

# Email Configuration (Optional - if not set, emails will be logged to console)
# For Gmail: use app-specific password (https://myaccount.google.com/apppasswords)
# For other providers: adjust SMTP_HOST and SMTP_PORT accordingly
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_app_specific_password
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
